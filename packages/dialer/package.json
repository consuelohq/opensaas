{
  "name": "@consuelo/dialer",
  "version": "0.0.1",
  "description": "Twilio-powered dialer with local presence and caller ID",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
