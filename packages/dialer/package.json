  "license": "MIT",
  "peerDependencies": {
    "twilio": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "twilio": {
      "optional": true
    }
  },
  "devDependencies": {
    "twilio": "^5.12.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
